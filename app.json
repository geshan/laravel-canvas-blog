{
  "name": "laravel-canvas-blog",
  "options": {
      "allow-unauthenticated": false,
      "memory": "512Mi",
      "cpu": "1",
      "port": 80,
      "http2": false,
      "concurrency": 1000,
      "max-instances": 5
  },  
  "hooks": {
      "prebuild": {
          "commands": [
              "echo 'prebuild'"
          ]
      },
      "postbuild": {
          "commands": [
              "echo 'postbuild'"
          ]
      },
      "precreate": {
          "commands": [
              "echo 'precreate'"
          ]
      },
      "postcreate": {
          "commands": [
              "echo 'postcreate'"
          ]
      }
  }
}